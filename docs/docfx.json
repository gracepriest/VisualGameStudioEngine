{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": ["**/bin/**", "**/obj/**", "**/*.Tests/**"],
          "src": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Visual Game Studio Engine",
      "_appName": "Visual Game Studio Engine",
      "_appFooter": "Visual Game Studio Engine Documentation",
      "_enableSearch": true,
      "_enableNewTab": true
    }
  }
}
